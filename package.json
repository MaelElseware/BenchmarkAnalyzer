{
  "name": "benchmark-analyzer",
  "version": "0.1.0",
  "homepage": "https://MaelElseware.github.io/BenchmarkAnalyzer",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.8.3",
    "firebase": "^11.4.0",
    "git": "^0.1.5",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "^3.0.1",
    "recharts": "^2.15.1",
    "tailwindcss": "^4.0.12",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "test": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts test",
    "eject": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts eject",
    "deploy": "gh-pages -d build",
    "predeploy": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.yourname.benchmarkanalyzer",
    "productName": "Benchmark Analyzer",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "electron.js",
      "preload.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis"
    },
    "fileAssociations": [
      {
        "ext": "log",
        "name": "Benchmark Log",
        "role": "Viewer"
      }
    ]
  },
  "devDependencies": {
    "electron": "^35.0.2",
    "electron-builder": "^25.1.8",
    "gh-pages": "^6.3.0"
  }
}
